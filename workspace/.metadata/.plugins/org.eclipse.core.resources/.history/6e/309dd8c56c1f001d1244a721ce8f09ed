package method;

class Test{
	
	public void disp() {
		System.out.println("non-static 메소드");
	}
	
	public static void output() {
		System.out.println("static 메소드");
	}
	
}
//----------------------
public class Method02 {
	
	public static void main(String[] args) {
		System.out.println("안녕하세요");

	}

}
