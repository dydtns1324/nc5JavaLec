package if_;

import java.util.Scanner;

public class If01 {

	public static void main(String[] args) {
		Scanner scan= new Scanner(System.in); //생성
		System.out.println("객체 = " + scan);
				
		int a= 35;
		
		String result = a >= 50 ? "a는 50보다 크거나 같다" : "a는 50보다 작다";
		System.out.println(result);
	}

}
