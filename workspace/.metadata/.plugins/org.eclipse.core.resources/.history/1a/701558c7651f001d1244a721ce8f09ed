package variable;

class Test {
	String name; //필드, 초기화(값=null)
	int age;
}
//-------------------
public class Variable0401 {

	public static void main(String[] args) {
		Test t = new Test(); //생성
		System.out.println("객체 t = " + t);
		System.out.println("이름 = " + Test.name);
		System.out.println("나이 = " + Test.age);

	}

}
